187)repeated-dna-sequences
class Solution:
    def findRepeatedDnaSequences(self, s: str) -> List[str]:
        seen=set()
        repeated=set()
        for i in range(len(s)-9):
            seq=s[i:i+10]
            if seq in seen:
                repeated.add(seq)
            else:
                seen.add(seq)
        return list(repeated)

        

680)valid-palindromr-ii

class Solution:
    def validPalindrome(self, s: str) -> bool:
        def is_palindrome(l, r):
            while l < r:
                if s[l] != s[r]:
                    return False
                l += 1
                r -= 1
            return True

        l, r = 0, len(s) - 1
        while l < r:
            if s[l] != s[r]:
                return is_palindrome(l+1, r) or is_palindrome(l, r-1)
            l += 1
            r -= 1
        return True
