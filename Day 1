2235. Add Two Integers

class Solution:
    def sum(self, num1: int, num2: int) -> int:
        return num1 + num2

283. Move Zeroes

class Solution:
    def getConcatenation(self, nums):
        return nums * 2

283. Move Zeroes

class Solution:
    def moveZeroes(self, nums):
        pos = 0 
        for num in nums:
            if num != 0:
                nums[pos] = num
                pos += 1
       
        for i in range(pos, len(nums)):
            nums[i] = 0
